   \n","    # Sort df_nan in accending order of the proportion of NaN\n","    df_nan = df_nan.sort_values(by='proportion', ascending=False).reset_index(drop=True)\n","    \n","    return df_nan"],"metadata":{"id":"afD6yz8AXRej","executionInfo":{"status":"ok","timestamp":1650833579690,"user_tz":240,"elapsed":150,"user":{"displayName":"Nate Ehat","userId":"01467513665981823629"}}},"execution_count":54,"outputs":[]},{"cell_type":"code","source":["# Call nan_checker on df\n","# See the implementation in pmlm_utilities.ipynb\n","df_nan = nan_checker(df)\n","\n","# Print df_nan\n","df_nan"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":488},"id":"z7tQwi3bXWW5","executionInfo":{"status":"ok","timestamp":1650833581372,"user_tz":240,"elapsed":134,"user":{"displayName":"Nate Ehat","userId":"01467513665981823629"}},"outputId":"cec4f135-efcb-4dfc-a759-0a56f396b5c2"},"execution_count":55,"outputs":[{"output_type":"execute_result","data":{"text/plain":["                var  proportion    dtype\n","0        pl_orbsmax    0.438863  float64\n","1    st_metallicity    0.350892  float64\n","2           pl_rade    0.298215  float64\n","3   st_surf_gravity    0.166167  float64\n","4           st_mass    0.135389  float64\n","5         pl_orbper    0.086857  float64\n","6    st_temp_eff_k2    0.081784  float64\n","7    st_temp_eff_k1    0.074484  float64\n","8     st_temp_eff_k    0.057472  float64\n","9         st_radius    0.052956  float64\n","10   sy_distance_pc    0.024343  float64\n","11       sy_gaiamag    0.020601  float64\n","12          sy_kmag    0.012218  float64\n","13          sy_vmag    0.011692  float64"],"text/html":["\n","  <div id=\"df-feb3490e-90e9-44a9-a796-f4454671fc7c\">\n","    <div class=\"colab-df-container\">\n","      <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe the